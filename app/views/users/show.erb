<h1> <%= @user.username %> </h1>

<%@pictures.each do |picture|%>
    <br>
    "<%= picture.title %>"
    <br>
    <%= link_to image_tag(picture.img_url), picture_path(picture)  %> <br>
    <br>
<%end%>

<%= link_to "View Other Users", users_path %>
